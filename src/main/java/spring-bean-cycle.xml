<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="car" class="com.spring.bean.cycle.Car" init-method="init" destroy-method="destory" p:name="audo"/>
	<!-- Spring的bean的生命周期 -->
	<!-- postProcessBeforeInitialization 在init方法之前执行 -->
	<!-- postProcessAfterInitialization 在init方法之后执行  -->
	<bean id="beanprosscer" class="com.spring.bean.cycle.MyBeanProcessor"></bean>
</beans>
